
<%= image_tag "/assets/icons/Dude@2x.png",:width=>"90",:height=>"90",:class => "pull-right" %>

<% @issue.issue_progesses.each do |issue_progess| %>
<div class="span8 detail-popovers" >
    <div class="popover left">
      <div class="arrow"></div>
      <h3 class="popover-title">
        <%= issue_progess.name %>
        <%= link_to '', issue_progess, method: :delete, data: { confirm: 'Are you sure?' },class: "text-right close fui-cross" %>
      </h3>
      <div class="popover-content">
        <p><%= issue_progess.remark %></p>
        
      </div>
    </div>
</div>
<% end %>
<script type="text/javascript">
    $(".popover").show();
</script>
